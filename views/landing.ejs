<%- include('partials/header', { title: "Dashboard" }) %>

<h1>Welcome, <%= username %>!</h1>

<% if (users) { %>
<h2>All Users</h2>
<ul>
  <% users.forEach((user) => { %>
  <li><%= user.username %> (<%= user.email %>)</li>
  <% }); %>
</ul>
<% } else { %> <% } %>

<form action="/logout" method="POST">
  <button type="submit">Logout</button>
</form>

<%- include('partials/footer') %>
